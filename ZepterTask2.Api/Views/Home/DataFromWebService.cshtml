@model IEnumerable<ZepterTask.Models.DTOs.OrderSummaryDto>

@{
    ViewData["Title"] = "Data from Web Service";
}

<h1>Data from Web Service</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Store ID</th>
            <th>Payment Method</th>
            <th>Total Net Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderId</td>
                <td>@order.StoreId</td>
                <td>@order.PaymentMethod</td>
                <td>@order.TotalNetPrice</td>
            </tr>
        }
        }
    </tbody>
</table>
